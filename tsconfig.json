{
    "compilerOptions": {
        "target": "ES2018",
        "module": "commonjs",
        "lib": [
            "esnext"
        ],
        "strict": true,
        "rootDir": ".",
        "outDir": "dist",
        "sourceMap": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "typeRoots": [
            "./node_modules/@types",
            "./generated/",
            "./src/types"
        ]
    },
    "exclude": [
        "node_modules/**/*",
        ".vscode/**/*"
    ],
}